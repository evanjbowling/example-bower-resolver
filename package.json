{
  "name": "example-bower-resolver",
  "version": "0.0.1",
  "description": "sample implementation of a bower pluggable resolver",
  "main": "src/index.js",
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha",
    "coveralls": "npm run coverage && cat coverage/coverage.lcov | ./node_modules/coveralls/bin/coveralls.js",
    "coverage-prep": "mkdir coverage && npm run instrument-prep",
    "coverage": "npm run coverage-prep && node ./node_modules/.bin/mocha -R mocha-lcov-reporter | sed 's|SF:|SF:src/|' > coverage/coverage.lcov && npm run coverage-cleanup",
    "coverage-cleanup": "npm run instrument-cleanup",
    "instrument-prep": "node node_modules/.bin/jscover src src-cov && mv src src-orig && mv src-cov src",
    "instrument-cleanup": "rm -rf src && mv src-orig src"
  },
  "keywords": [
    "bower-resolver",
    "example"
  ],
  "author": "Evan Bowling",
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^2.12.0",
    "jscover": "^1.0.0",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mock-require": "^2.0.1"
  },
  "bugs": "https://github.com/evanjbowling/example-bower-resolver/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/evanjbowling/example-bower-resolver"
  },
  "files": [
    "src/",
    "README.md",
    "LICENSE",
    "package.json"
  ]
}
